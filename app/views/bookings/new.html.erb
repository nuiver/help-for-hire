<h1>New Booking for id <%= @service.id %></h1>

<p><%= @service.name %><p>

<% if can? :create, Booking %>
  <%= render 'form', booking: @booking, service_id: params[:format] %>
<% else %>
  <p>Sign in to create a booking</p>
<% end %>

<%= link_to 'Back', bookings_path %>

